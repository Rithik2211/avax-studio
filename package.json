{
  "name": "subnet-studio",
  "version": "1.0.0",
  "description": "No-Code Avalanche Subnet Builder - Visual subnet creation and deployment platform",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && npm run dev",
    "build": "cd frontend && npm run build",
    "start": "cd backend && npm start",
    "install:all": "npm install && cd frontend && npm install && cd ../backend && npm install",
    "setup": "npm run install:all && docker-compose up -d"
  },
  "keywords": [
    "avalanche",
    "subnet",
    "blockchain",
    "no-code",
    "defi",
    "web3"
  ],
  "author": "Subnet Studio Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
